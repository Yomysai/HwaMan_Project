package fr.freestyle.hawman.Bean;

/**
 * @author aly_d
 *
 */
@Entity
@Table(name = "users")
public class User {
    @Id
    @GeneratedValue(strategy = GenerationType.IDENTITY)
    private Long user_id;

    private String username;
    private String password;
    private String email;

    // Getters and setters
}
